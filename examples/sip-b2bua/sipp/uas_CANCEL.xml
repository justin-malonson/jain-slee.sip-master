<?xml version="1.0" encoding="ISO-8859-1" ?> <!DOCTYPE scenario SYSTEM "sipp.dtd">


<scenario name="Basic UAS responder">

	<recv request="INVITE" rrs="true">
	  <action>
    		<ereg regexp=".*" search_in="hdr" header="CSeq:" check_it="true" assign_to="1" /> 
	  </action>
  	</recv>

	<send>
   <![CDATA[

     SIP/2.0 180 Ringing
     [last_Via:]
     [last_From:]
     [last_To:];tag=[call_number]
     [last_Call-ID:]
     [last_CSeq:]
     [last_Record-Route:]
     [routes]
     Contact: <sip:[local_ip]:[local_port];transport=[transport]>
     Content-Length: 0

   ]]>
	</send>
	 
	<recv request="CANCEL" timeout="10000" />

 <send>
   <![CDATA[

     SIP/2.0 200 OK
     [last_Via:]
     [last_From:]
     [last_To:];tag=[call_number]
     [last_Call-ID:]
     [last_CSeq:]
     [last_Record-Route:]
     [routes]
     Content-Length: 0

   ]]>
 </send>
 
 <send>
   <![CDATA[

     SIP/2.0 487 Request Terminated
     [last_Via:]
     [last_From:]
     [last_To:]tag=[call_number]
     [last_Call-ID:]
     CSeq :[$1]
     [last_Record-Route:]
     [routes]
     Contact: <sip:[local_ip]:[local_port];transport=[transport]>
     Content-Length: 0

   ]]>
 </send>
  
 <recv request="ACK" timeout="10000" />
 
 <pause milliseconds="30000" />

</scenario> 
